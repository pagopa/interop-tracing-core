{
  "name": "pagopa-interop-tracing-operations-client",
  "private": true,
  "version": "1.0.0",
  "description": "PagoPA tracing operations client",
  "main": "dist",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start": "node --watch --no-warnings --loader ts-node/esm ./src/index.ts",
    "build": "tsc",
    "check": "tsc --project tsconfig.check.json",
    "generate-client": "mkdir -p ./src/model/generated && pnpm openapi-zod-client '../../tracing-operations/open-api/api-tracing-operations-interop-v1.yaml' -o './src/model/generated/client.ts'",
    "clean-generated": "pnpm exec rm ./src/model/generated/client.ts"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "20.3.1",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "prettier": "3.0.3",
    "ts-node": "10.9.2",
    "typescript": "5.5.2"
  },
  "dependencies": {
    "@zodios/core": "10.9.6",
    "@zodios/express": "10.6.1",
    "openapi-zod-client": "1.18.1",
    "ts-pattern": "5.1.2",
    "zod": "3.23.8"
  }
}