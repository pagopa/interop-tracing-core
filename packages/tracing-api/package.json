{
    "name": "pagopa-interop-tracing-api",
    "version": "1.0.0",
    "description": "PagoPA tracing api",
    "main": "dist",
    "type": "module",
    "scripts": {
        "test": "vitest --config ./test/vitest.config.ts",
        "test:it": "vitest db --config ./test/vitest.config.ts",
        "lint": "eslint . --ext .ts,.tsx -c .eslintrc",
        "lint:autofix": "eslint . --ext .ts,.tsx --fix",
        "format:check": "prettier --check src",
        "format:write": "prettier --write src",
        "start": "node --watch --no-warnings --loader ts-node/esm -r 'dotenv-flow/config' ./src/index.ts",
        "build": "tsc",
        "generate-model": "mkdir -p ./src/model/generated && pnpm openapi-zod-client './open-api/api-external-interop-v1.yaml' -o './src/model/generated/api.ts' && node --no-warnings --loader ts-node/esm ./multerPatchZodios.ts",
        "clean-generated": "pnpm exec rm ./src/model/generated/api.ts"
    },
    "keywords": [],
    "author": "",
    "license": "Apache-2.0",
    "devDependencies": {
        "@types/cors": "2.8.17",
        "@types/dotenv-flow": "3.3.3",
        "@types/express": "4.17.17",
        "@types/node": "20.14.1",
        "@types/supertest": "6.0.2",
        "@types/uuid": "9.0.2",
        "supertest": "6.3.4",
        "ts-node": "10.9.2",
        "typescript": "5.5.2",
        "vitest": "0.33.0"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "3.588.0",
        "@tsconfig/node-lts": "20.1.0",
        "@types/busboy": "1.5.4",
        "@types/express-fileupload": "1.5.0",
        "@types/multer": "1.4.11",
        "@zodios/core": "10.9.6",
        "@zodios/express": "10.6.1",
        "@zodios/plugins": "10.6.0",
        "aws-sdk": "2.657.0",
        "axios": "1.6.7",
        "busboy": "1.6.0",
        "cors": "2.8.5",
        "dotenv-flow": "4.1.0",
        "express": "4.18.2",
        "express-fileupload": "1.5.0",
        "helmet": "7.1.0",
        "multer": "1.4.5-lts.1",
        "openapi-zod-client": "1.18.1",
        "pagopa-interop-tracing-commons": "workspace:*",
        "pagopa-interop-tracing-models": "workspace:*",
        "pagopa-interop-tracing-operations-client": "workspace:*",
        "ts-pattern": "5.0.1",
        "uuid": "9.0.0",
        "zod": "3.23.8"
    }
}