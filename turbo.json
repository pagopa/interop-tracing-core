{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "start": {
      "persistent": true,
      "cache": false,
      "dependsOn": [
        "^build",
        "generate-client"
      ]
    },
    "build": {
      "dependsOn": [
        "generate-client",
        "^build"
      ],
      "outputs": [
        "dist"
      ]
    },
    "generate-client": {
      "dependsOn": [
        "^generate-client"
      ]
    },
    "clean-generated": {
      "dependsOn": [
        "^clean-generated"
      ]
    },
    "test": {
      "dependsOn": [
        "build",
        "^test"
      ]
    },
    "test:it": {
      "dependsOn": [
        "build",
        "^test:it"
      ]
    },
    "lint": {},
    "lint:autofix": {},
    "format:check": {},
    "format:write": {}
  },
  "globalDependencies": [
    "tsconfig.json"
  ]
}
